{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["config:base"],
  "regexManagers": [
    {
      "ignorePaths": [],
      "fileMatch": [
        ".*testcontainers\\.properties"
      ],
      "matchStrings": [
        "[a-zA-Z]+=(?<depName>.*?):(?<currentValue>.*)"
      ],
      "datasourceTemplate": "docker",
      "enabled": true
    }
  ]
}
